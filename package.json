{
  "name": "cardboard_express_shop",
  "version": "1.0.0",
  "description": "Official Cardboard Express Marketplace",
  "engines": {
    "node": "10.15.0"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Tunde Adepitan",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "bcrypt": "^5.0.0",
    "config": "^3.3.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "google-distance-matrix": "^1.1.1",
    "gridfs-stream": "^1.1.1",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^3.3.0",
    "react-router-dom": "^5.2.0",
    "react-youtube": "^7.13.0",
    "stripe": "^7.15.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.19.4"
  }
}
